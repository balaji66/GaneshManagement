<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.durga.balaji66.ganeshmanagement">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity
            android:name=".SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="LogIn"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".GamesListActivity"
            android:label="List Of Games"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AnxietyRegisteredPlayersActivity"
            android:label="Registered Players"
            android:screenOrientation="portrait" />
        <activity
            android:name=".BalloonRegisteredPlalyersActivity"
            android:label="Registered Players"
            android:screenOrientation="portrait" />
        <activity
            android:name=".BubbleRegisteredPlayerActivity"
            android:label="Registered Players"
            android:screenOrientation="portrait" />
        <activity android:name=".ListOfRegisteredPlayersActivity" android:label="List of Registered Players" android:screenOrientation="portrait"/>

    </application>

</manifest>